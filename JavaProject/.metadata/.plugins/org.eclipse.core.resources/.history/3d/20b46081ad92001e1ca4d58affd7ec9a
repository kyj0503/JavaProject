package tp.module;

// Data Transfer Object

public class ToDoDTO {
    private String data;

    public ToDoDTO() {}

    public ToDoDTO(String data) {
        super();
        this.data = data;
    }

    public String getData() {
        return data;
    }

    public void setData(String data) {
        this.data = data;
    }
    
    @Override
    public String toString() {
        return data;
    }

	public String getId() {
		return null;
	}
}